<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3"/>
        <add key="github" value="https://nuget.pkg.github.com/qeqig/index.json" />
    </packageSources>
    <packageSourceMapping>
        <packageSource key="nuget.org">
            <package pattern="*">
            </package>
        </packageSource>
        <packageSource key="github">
            <package pattern="qeqig.*">
            </package>
        </packageSource>
    </packageSourceMapping>

</configuration>